{
   "plugins" : [
      "NeatHTMLFeatures",
      "NeatCanvasFeatures",
      "HideTrackLabels"
   ],
   "formatVersion" : 1,
   "names" : {
      "url" : "http://agrjbrowse.s3-website-us-east-1.amazonaws.com/SGD/yeast/names/",
      "type" : "Hash"
   },
   "tracks" : [
      {
         "category" : "Reference sequence",
         "type" : "SequenceTrack",
         "key" : "Reference sequence",
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "urlTemplate" : "http://agrjbrowse.s3-website-us-east-1.amazonaws.com/SGD/yeast/seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA",
         "chunkSize" : 20000
      },
      {
         "type" : "CanvasFeatures",
         "style" : {
            "color" : "function( featureObject, variableName, glyphObject, trackObject ) {  return '#ffcccc'; }",
            "label" : "gene,name,id",
            "className" : "feature",
            "description" : "description,note"
         },
         "label" : "All Genes",
         "trackType" : "CanvasFeatures",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "labelTranscripts" : false,
         "key" : "All Genes",
         "maxHeight" : 3000,
         "compress" : 1,
         "urlTemplate" : "http://agrjbrowse.s3-website-us-east-1.amazonaws.com/SGD/yeast/tracks/All Genes/{refseq}/trackData.jsonz",
         "fmtDetailValue_Name" : "function(name, feature) {if(feature.get('type')=='gene') {return '<a href=\"http://www.yeastgenome.org/locus/'+name+'/overview\">'+name+'</a>';} else { return name;}}"
      }
   ],
   "dataset_id" : "yeast",
}
