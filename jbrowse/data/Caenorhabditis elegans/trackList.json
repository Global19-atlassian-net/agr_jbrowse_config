{
   "tracks" : [
      {
         "type" : "SequenceTrack",
         "seqType" : "dna",
         "chunkSize" : 20000,
         "key" : "Reference sequence",
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "urlTemplate" : "seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA"
      },
      {
         "label" : "All Genes",
         "style" : {
            "color" : "{geneColor}",
            "className" : "feature",
            "label":"{geneLabel}",
            "height" : "{geneHeight}"
         },
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "urlTemplate" : "tracks/All Genes/{refseq}/trackData.json",
         "compress" : 0,
         "trackType" : "CanvasFeatures",
         "key" : "All Genes",
         "type" : "CanvasFeatures",
         "glyph" : "{geneGlyph}",
         "onClick":{
            "title":"Click to open in a full window:",
            "url":"function(track,f) {var curie = f.get('curie'); return 'http://dev.alliancegenome.org/gene/'+curie; }",
            "action":"iframeDialog",
            "label":"Feature at AGR"
         }
      }
   ],
   "names" : {
      "url" : "names/",
      "type" : "Hash"
   },
   "include" : ["../functions.conf"],
   "formatVersion" : 1
}
