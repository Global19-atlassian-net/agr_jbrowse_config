{
   "formatVersion" : 1,
   "names" : {
      "type" : "Hash",
      "url" : "names/"
   },
   "tracks" : [
      {
         "type" : "SequenceTrack",
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "key" : "Reference sequence",
         "urlTemplate" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/SGD/yeast/seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA",
         "compress" : 1,
         "chunkSize" : 80000
      },
      {
         "trackType" : "CanvasFeatures",
         "compress" : 0,
         "impliedUTRs" : true,
         "label" : "All Genes",
         "urlTemplate" : "tracks/All Genes/{refseq}/trackData.json",
         "labelTranscripts" : false,
         "fmtDetailValue_Name" : "function(name, feature) {if(feature.get('type')=='gene') {return '<a href=\"http://www.yeastgenome.org/locus/'+name+'/overview\">'+name+'</a>';} else { return name;}}",
         "key" : "All Genes",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "glyph" : "{geneGlyph}",
         "style" : {
            "color" : "{geneColor}",
            "className" : "feature",
            "description" : "display",
            "height" : "{geneHeight}",
            "label" : "gene,name,id"
         },
         "onClick" : {
            "title" : "Click to open in a full window:",
            "label" : "function() {var note = this.feature.get('Note'); return note;  }",
            "url" : "{curieUrl}",
            "action" : "iframeDialog"
         },
         "type" : "CanvasFeatures",
         "maxHeight" : 3000
      }
   ],
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "dataset_id" : "yeast",
   "include" : [
      "../functions.conf"
   ]
}
