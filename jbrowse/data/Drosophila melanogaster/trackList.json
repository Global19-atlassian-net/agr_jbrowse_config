{
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "names" : {
      "url" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/FlyBase/fruitfly/names/",
      "type" : "Hash"
   },
   "tracks" : [
      {
         "chunkSizeLimit" : 5000000,
         "unsafeMouseover" : true,
         "unsafePopup" : true,
         "style" : {
             "label" : "{variationLabel}"
         },
         "onClick" : {
            "label" : "{variantsBalloon}",
            "url" : "{variationCurieUrl}",
            "title" : "Put a title here",
            "action" : "iframeDialog"
         },
         "label" : "Phenotypic Variants",
         "type" : "CanvasVariants",
         "urlTemplate" : "R6.27-2.2.0.vcf.gz",
         "storeClass" : "JBrowse/Store/SeqFeature/VCFTabix",
         "key" : "Phenotypic Variants"
      },
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "chunkSize" : 80000,
         "urlTemplate" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/FlyBase/fruitfly/seq/{refseq_dirpath}/{refseq}-",
         "label" : "DNA",
         "type" : "SequenceTrack",
         "compress" : 1,
         "key" : "Reference sequence"
      },
      {
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "height" : "{geneHeight}",
            "className" : "feature"
         },
         "key" : "All Genes",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : "CanvasFeatures",
         "urlTemplate" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/FlyBase/fruitfly/tracks/All Genes/{refseq}/trackData.jsonz",
         "compress" : 1,
         "type" : "CanvasFeatures",
         "label" : "All Genes",
         "maxHeight" : 3000
      }
   ],
   "include" : [
      "../functions.conf"
   ],
   "dataset_id" : "fly",
   "formatVersion" : 1
}
