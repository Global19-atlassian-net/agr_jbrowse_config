{
   "tracks" : [
      {
         "chunkSize" : 80000,
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "urlTemplate" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/MGI/mouse/seq/{refseq_dirpath}/{refseq}-",
         "type" : "SequenceTrack",
         "label" : "DNA",
         "compress" : 1,
         "key" : "Reference sequence"
      },
      {
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "height" : "{geneHeight}",
            "label" : "function(f) {var curie = f.get('curie'); var name = f.get('Name'); if (curie) {return name + ' (' + curie + ')';} return name;  }",
            "className" : "feature"
         },
         "key" : "All Genes",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : "CanvasFeatures",
         "urlTemplate" : "tracks/All%20Genes/{refseq}/trackData.jsonz",
         "type" : "CanvasFeatures",
         "label" : "All Genes",
         "maxHeight" : 3000
      }
   ],
   "names" : {
      "url" : "names/",
      "type" : "Hash"
   },
   "plugins" : [
      "HideTrackLabels"
   ],
   "include" : [
      "../functions.conf"
   ],

   "dataset_id" : "mouse",
   "formatVersion" : 1
}
