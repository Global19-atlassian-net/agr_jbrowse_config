{
   "names" : {
      "type" : "Hash",
      "url" : "https://s3.amazonaws.com/agrjbrowse2/WormBase/c_elegans_PRJNA13758/names/"
   },
   "tracks" : [
     {
         "trackType" : "CanvasFeatures",
         "style" : {
            "className" : "feature",
            "label" : "function(f) {var type =  f.get('Note'); if (typeof type ==='undefined') {return ' ';} return type;}",
            "height" : 7,
            "color" : "function(f) {var type =  f._parent.get('predictiontype'); if (type == 'tmhmm') {return 'magenta';} if (type == 'seg') {return 'lightseagreen';} if (type == 'signalp') {return 'aquamarine'} if (type == 'ncoils') {return 'chartreuse';} if (type == 'pfam') {return 'lightsalmon';} return 'purple'; }",
            "description" : "function(f) {var aa_range = f.get('range'); var exons = f.get('exons'); var label = f.get('predictiontype'); if (aa_range) {label = label + '; amino acids: '+aa_range;} if (exons) {label = label + '; exons: '+exons;} if(label) {return label;} return 'No prediction type provided';}"
         },
         "metadata" : {
            "shortInfo" : "This track shows the extent of predicted protein motifs. Note these spans correspond to amino acid coordinates interpolated onto the physical map.  Included are signal peptide (signalp), coiled coil (ncoils) and transmembrane (tmhmm) domains, regions of low complexity (seg), and Pfam annotated motif homologies."
         },
         "type" : "CanvasFeatures",
         "label" : "Protein motifs",
         "urlTemplate" : "https://www.wormbase.org/tools/genome/jbrowse/data/tracks/Protein motifs/{refseq}/trackData.jsonz",
         "unsafePopup" : true,
         "key" : "Protein motifs",
         "subParts" : "polypeptide_motif_part",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "maxExportSpan" : 50000000,
         "glyph" : "JBrowse/View/FeatureGlyph/Segments",
         "compress" : 1,
         "unsafeMouseover" : true,
      },
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "key" : "Reference sequence",
         "label" : "DNA",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/WormBase/c_elegans_PRJNA13758/seq/{refseq_dirpath}/{refseq}-",
         "seqType" : "dna",
         "type" : "SequenceTrack",
         "compress" : 1,
         "chunkSize" : 80000
      },
      {
         "label" : "All Genes",
         "maxHeight" : 3000,
         "trackType" : "CanvasFeatures",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "label" : "Feature at AGR",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "label" : "{geneLabel}",
            "height" : "{geneHeight}",
            "className" : "feature"
         },
         "compress" : 1,
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse2/WormBase/c_elegans_PRJNA13758/tracks/All Genes/{refseq}/trackData.jsonz",
         "type" : "CanvasFeatures",
         "key" : "All Genes"
      },
      {
         "chunkSizeLimit" : 5000000,
	 "unsafeMouseover" : true,
	 "unsafePopup" : true,
	 "style" : {
            "color" : "{variationColor}",
             "height" : 8,
	     "description" : "{variationDescription}",
             "label" : "{variationLabel}"
         },
         "glyph" : "{variationGlyph}",
	 "onClick" : {
            "label" : "{variantsBalloon}",
            "url" : "{variationCurieUrl}",
            "title" : "Put a title here",
            "action" : "iframeDialog"
         },
	 "metadata" : {
            "about" : "Phenotypic variants. Features are colored according to the Variant Effect Predictor's predicted impact: red for high, purple for modifier, gold/yellow for medium, and cyan for low."
	 },
         "label" : "Variants",
         "type" : "CanvasVariants",
         "urlTemplate" : "https://s3.amazonaws.com/agrjbrowse/VCF/VCF_WBcel235_20.vcf.gz",
         "storeClass" : "JBrowse/Store/SeqFeature/VCFTabix",
         "key" : "Variants"
      },
      {
         "label" : "UCSC Conservation",
         "variance_band" : 0,
         "unsafeMouseover" : true,
         "autoscale" : "local",
         "type" : "JBrowse/View/Track/Wiggle/XYPlot",
         "urlTemplate" : "https://s3.amazonaws.com/wormbase-modencode/UCSC/ce11.phastCons26way.bw",
         "unsafePopup" : true,
         "key" : "UCSC Conservation",
         "metadata" : {
            "shortInfo" : "UCSC Nematode conservation calculated across nematode 26 species. For more information about these data, see the <a href='http://hgdownload.soe.ucsc.edu/goldenPath/ce11/phastCons26way/'>README at UCSC</a>. "
         },
         "style" : {
            "height" : 20,
            "pos_color"  : "grey"
         },
         "storeClass" : "JBrowse/Store/SeqFeature/BigWig"
      }
   ],
   "dataset_id" : "worm",
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "include" : [
      "../functions.conf"
   ],
   "formatVersion" : 1
}
