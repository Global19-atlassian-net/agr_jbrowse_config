{
   "names" : {
      "type" : "Hash",
      "url" : "names/"
   },
   "tracks" : [
      {
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "key" : "Reference sequence",
         "label" : "DNA",
         "urlTemplate" : "http://agrjbrowse2.s3-website-us-east-1.amazonaws.com/WormBase/c_elegans_PRJNA13758/seq/{refseq_dirpath}/{refseq}-",
         "seqType" : "dna",
         "type" : "SequenceTrack",
         "compress" : 1,
         "chunkSize" : 80000
      },
      {
         "label" : "All_Genes",
         "trackType" : "CanvasFeatures",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "glyph" : "{geneGlyph}",
         "impliedUTRs" : true,
         "onClick" : {
            "url" : "{curieUrl}",
            "title" : "Click to open in a full window:",
            "label" : "Feature at AGR",
            "action" : "iframeDialog"
         },
         "style" : {
            "color" : "{geneColor}",
            "label" : "{geneLabel}",
            "height" : "{geneHeight}",
            "className" : "feature"
         },
         "urlTemplate" : "tracks/All Genes/{refseq}/trackData.json",
         "type" : "CanvasFeatures",
         "key" : "All Genes"
      }
   ],
   "dataset_id" : "worm",
   "plugins" : [
      "NeatHTMLFeatures",
      "HideTrackLabels"
   ],
   "include" : [
      "../functions.conf"
   ],
   "formatVersion" : 1
}
